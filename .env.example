OPENAI_API_KEY=sk-proj-1234567890abcdefghijklmnopqrstuvwxyz1234567890ab
PORT=3000

# Server secret for RE-ARC dataset generation (32+ character random string)
# Used to derive internal seeds that prevent dataset regeneration without server access
RE_ARC_SEED_PEPPER=your-secret-pepper-here-minimum-32-characters-required

# LLM Council configuration (multi-model consensus evaluation)
# Council runs as subprocess (like Saturn/Grover) - no separate service needed
# Timeout for council deliberation in milliseconds (default: 180000 = 3 minutes)
COUNCIL_TIMEOUT_MS=180000

# OpenRouter API key - required for LLM Council (uses OpenRouter for multi-model queries)
# OPENROUTER_API_KEY=sk-or-v1-...