{
  "description": "Template configuration for this project. See https://www.codebuff.com/config for all options.",
  "startupProcesses": [
    {
      "name": "dev",
      "command": "npm run dev",
      "stdoutFile": "logs/dev.log"
    }
  ],
  "fileChangeHooks": [
    {
      "name": "typecheck",
      "command": "npx tsc --noEmit",
      "filePattern": "**/*.{ts,tsx}",
      "enabled": true
    }
  ],
  "addedSpawnableAgents": [
    "mark-barney/edgar-the-engineer@0.0.4",
    "mark-barney/benny@0.0.5",
    "mark-barney/mark@0.2.6",
    "codebuff/implementation-planner@0.0.2"
  ]
}
